citHeader("To cite reda in publications use:")

bibentry(
    key = "reda-package",
    bibtype = "Manual",
    title = "{reda}: {R}ecurrent Event Data Analysis",
    author = personList(as.person("Wenjie Wang"),
                        as.person("Haoda Fu"),
                        as.person("Jun Yan")),
    year = "2021",
    url = "https://github.com/wenjie2wang/reda",
    note = "{R} package version 0.5.3"
)
